<p id="notice"><%= notice %></p>

<h1>Wine List</h1>

<%= form_tag products_path, :method => 'get', :id => "products_search" do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search", :name => nil %>
  </p>
<% end %>
<div id="products"><%= render 'list' %></div>
